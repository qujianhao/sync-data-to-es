<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.miao.cloud.mapper.HealthUserPhoneNumberAbnormalIndicatorMapper">

    <select id="getListById" resultType="com.miao.cloud.bean.HealthUserPhoneNumberAbnormalIndicator">
        select
               *
        from
            health_user_phone_number_abnormal_indicator
        where
                id  &lt; #{id}  order by id desc  limit 500

    </select>


</mapper>
